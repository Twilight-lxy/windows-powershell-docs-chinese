msgstr "### PTR"

msgid "### NS"
msgstr "### NS"

msgid "### MX"
msgstr "### MX"

msgid "### ISDN"
msgstr "### ISDN"

msgid "### HINFO"
msgstr "### HINFO"

msgid "### DNAME"
msgstr "### DNAME"

msgid "### DHCID"
msgstr "### DHCID（动态主机配置标识）"

msgid "### CNAME"
msgstr "### CNAME（ Canonical Name Resolution）"

