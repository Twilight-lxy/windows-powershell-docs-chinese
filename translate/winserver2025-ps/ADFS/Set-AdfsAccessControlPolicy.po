msgid "# Set-AdfsAccessControlPolicy"
msgstr "# 设置 AdfsAccessControlPolicy"

msgid "## SYNOPSIS"
msgstr "## 概要"

msgid "Modifies an AD FS access control policy."
msgstr "修改 AD FS 访问控制策略。"

msgid "## SYNTAX"
msgstr "## 语法"

msgid "### IdentifierName"
msgstr "### IdentifierName"

msgid "### Identifier"
msgstr "### 标识符"

